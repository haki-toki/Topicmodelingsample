   "source": [
    "def normalized_topic(topic_sum_df):\n",
    "    for i in range(len(topic_sum_df)):\n",
    "        topic_sum_df.iloc[i] = topic_sum_df.iloc[i] / topic_sum_df.iloc[i, -1]\n",
    "    return topic_sum_df\n",
    "\n",
    "##def 실행 - normalizing\n",
    "weverse_onlytop = normalized_topic(weverse_onlytop)\n",
    "weverse_onlytop.reset_index(inplace=True)\n",
    "weverse_onlytop.head()"
   ]
