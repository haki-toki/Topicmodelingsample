    "import nltk\n",
    "import re\n",
    "#소문자로 변환-> stopwords가 소문자이므로\n",
    "# 소문자로 변환 후 영어빼고 다 제거\n",
    "lowered_doc = []\n",
    "idx=0\n",
    "for i in tqdm(text['content']):\n",
    "    t = i.lower()\n",
    "    t= re.sub(\"[^a-z\\s]\",\"\",t)\n",
    "    if t!='':\n",
    "        lowered_doc.append(t)\n",
    "    else:\n",
    "        lowered_doc.append(' ')\n",
    "    idx+=1\n",
    "    \n",
    "text['content'] = lowered_doc\n",
    "text['content']"
   ]
