    "#리뷰별로 문장나누기\n",
    "new_review=pd.DataFrame(columns=['content','score'])\n",
    "\n",
    "\n",
    "\n",
    "k=0\n",
    "for i in tqdm(range(len(weverse))):\n",
    "    for j in range(len(body_split[i])-1):\n",
    "        new_review.loc[k]=[body_split[i][j],weverse['score'][i]]\n",
