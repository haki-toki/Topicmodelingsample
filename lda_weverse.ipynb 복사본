   "source": [
    "import nltk\n",
    "words = set(nltk.corpus.words.words())\n",
    "non_eng = []\n",
    "\n",
    "for i in text['content']:\n",
    "    new = \" \".join(w for w in nltk.wordpunct_tokenize(i) \n",
    "             if w in words or not w.isalpha())\n",
    "    non_eng.append(new)\n",
    "\n",
    "text['content'] = non_eng\n",
    "text['content']"
   ]
